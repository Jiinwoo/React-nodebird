{
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx":true
        }
    },
    "env" : {
        "browser": true,
        "node":true,
        "es6":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules":{
        "no-underscore-dangle":"off",
        "react/forbid-prop-types":"off",
        "linebreak-style": 0,
        "indent":0,
        "react/jsx-filename-extension":"off"
    }
}